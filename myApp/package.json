{
  "name": "valoriza_app",
  "version": "0.0.1",
  "author": "Wedsley Fernando",
  "homepage": "wedsleyfernando@gmail.com",
  "main": "src/electron-starter.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-serve": "ng build --prod && electron .",
    "mac-release": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/favicon.png --prune=true --out=release-builds",
    "windows-release": "electron-packager . ValorizaApp --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='ValorizaApp'",
    "linux-release": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icon.png --prune=true --out=release-builds",
    "mac-build": "electron-installer-dmg ./release-builds/ionic-desktop-darwin-x64/ionic-desktop.app builds/ionic-desktop",
    "windows-build": "node src/windows-build.js"
  },
  "private": true,
